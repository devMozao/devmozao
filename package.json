    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "commit"
  },
  "engines": {
    "node": "^18.x.x",
    "npm": "^9.x.x"
  },
  "dependencies": {
    "@emotion/react": "^11.x",
    "@mantine/core": "^6.x",
