    "@tabler/icons-react": "^2.18.0",
    "@tanstack/react-query": "^4.x",
    "@tanstack/react-query-devtools": "^4.x",
    "axios": "^1.x",
    "inter-ui": "^3.x",
    "react": "^18.x",
    "react-dom": "^18.x",
    "react-router-dom": "^6.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.x",
    "@commitlint/config-conventional": "^17.x",
    "@commitlint/prompt-cli": "^17.x",
